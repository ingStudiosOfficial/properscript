{
  "name": "properscript",
  "displayName": "ProperScript",
  "description": "Syntax highlighting and IntelliSense for ProperScript - JavaScript, but proper.",
  "version": "1.0.0",
  "publisher": "ingStudiosOfficial",
  "icon": "images/ProperScript_Logo.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "properscript",
    "javascript",
    "transpiler",
    "british",
    "syntax"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ingStudiosOfficial/properscript.git"
  },
  "bugs": {
    "url": "https://github.com/ingStudiosOfficial/properscript/issues"
  },
  "homepage": "https://github.com/ingStudiosOfficial/properscript#readme",
  "license": "Apache-2.0",
  "activationEvents": [
    "onLanguage:properscript"
  ],
  "main": "./extension.cjs",
  "contributes": {
    "languages": [
      {
        "id": "properscript",
        "aliases": ["ProperScript", "properscript"],
        "extensions": [".ps"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/ProperScript_Logo.png",
          "dark": "./images/ProperScript_Logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "properscript",
        "scopeName": "source.properscript",
        "path": "./syntaxes/properscript.tmLanguage.json"
      }
    ]
  }
}