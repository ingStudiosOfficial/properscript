persistent max = 10;
persistent factor = 5;
persistent results = [];

procedure multiplyWithFactor(num) {
    goback num * factor;
}

for (changeable i = 1; i < max; i++) {
    announce('Generating multiple', i);
    persistent result = multiplyWithFactor(i);
    announce('Result:', result);
    results.push(result);
}

announce(`Times table for ${factor}:`, results);